{
  "name": "mattix",
  "version": "1.0.0",
  "main": "server/server.js",
  "type": "commonjs",
  "license": "MIT",
  "scripts": {
    "start": "node .",
    "server": "nodemon lint .",
    "build": "npm-run-all clean lint tsc copy-assets",
    "dev:start": "npm-run-all build start",
    "dev": "PORT=5000 API_MAX=25 NODE_ENV=development nodemon --watch src -e ts --exec npm run dev:start",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "clean": "rimraf server/*",
    "copy-assets": "npx shelljs tools/copyAssets.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "mattix",
    "mattix.com"
  ],
  "author": "Austin Mattix",
  "description": "Mattix PWA",
  "engines": {
    "node": ">=18.x"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cli-color": "^2.0.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "figlet": "^1.5.2",
    "graphql": "^16.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.5",
    "multer": "^1.4.5-lts.1",
    "node-2fa": "^2.0.3",
    "nodemailer": "^6.8.0",
    "uuid": "^9.0.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cli-color": "^2.0.2",
    "@types/colors": "^1.2.1",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.14",
    "@types/express-graphql": "^0.9.0",
    "@types/figlet": "^1.5.5",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.6",
    "@types/shelljs": "^0.8.11",
    "@types/uuid": "^8.3.4",
    "fs-extra": "^10.1.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3"
  }
